<div class="contents_row">
  <h4><strong><%= @user.nickname %></strong>さんのユーザーページです。</h4>
  <div class="image" style="background-image: url(<%= @user.avatar.url(:medium) %>);"></div>
  <h3><%= @user.name %><h3>
  <h3><%= @user.name_kana %></h3>
  <h3><%= @user.nickname %></h3>
  <h3><%= @user.gender %></h3>
  <h3><%= @user.country %></h3>
  <h3><%= @user.duration %></h3>
  <h3><%= @user.return_date %></h3>
  <h3><%= @user.introduction %></h3>
  <h3><a href="<%= @user.facebook %>"><%= @user.facebook %></a></h3>
  <h3><a href="<%= @user.twitter %>"><%= @user.twitter %></a></h3>
  <h3><%= @user.university %></h3>
  <h3><%= @user.faculty %></h3>
</div>